{
    "name":"Coursera Content Source Grabber",
    "version":"1.0",
    "description":"coursera content source grabber",
    "permissions":["storage", "declarativeContent", "activeTab", "*://*/*"],
    "background":{
        "scripts":["background.js"],
        "persistent":false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "images/a-icon-16.png",
          "32": "images/a-icon-32.png",
          "48": "images/a-icon-48.png",
          "128": "images/a-icon-128.png"
        }
    },
    "icons": {
        "16": "images/a-icon-16.png",
        "32": "images/a-icon-32.png",
        "48": "images/a-icon-48.png",
        "128": "images/a-icon-128.png"
      },
    "manifest_version":2
}